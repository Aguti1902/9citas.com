{
  "name": "9citas-backend",
  "version": "1.0.0",
  "description": "Backend API para 9citas.com",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "start:prod": "prisma db push --skip-generate --accept-data-loss && node dist/index.js"
  },
  "prisma": {
    "seed": "tsx src/prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@prisma/client": "^5.7.1",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^7.0.4",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^7.0.11",
    "openai": "^6.9.1",
    "prisma": "^5.7.1",
    "socket.io": "^4.6.0",
    "stripe": "^20.0.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "tsx": "^4.7.0"
  }
}
